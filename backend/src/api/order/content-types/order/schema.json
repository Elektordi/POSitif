{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "decimal",
      "required": true
    },
    "refund": {
      "type": "boolean",
      "required": true
    },
    "payment_method": {
      "type": "string",
      "required": true
    },
    "payment_infos": {
      "type": "string",
      "required": false
    },
    "payment_timestamp": {
      "type": "datetime",
      "required": true
    },
    "uid": {
      "type": "uid",
      "required": true
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store"
    },
    "lines": {
      "type": "component",
      "repeatable": true,
      "component": "order.order-line",
      "required": true
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "order"
    }
  }
}
